{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm ci && npx prisma generate",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "PRISMA_GENERATE_SKIP_AUTOINSTALL": "true"
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1",
      "PRISMA_GENERATE_SKIP_AUTOINSTALL": "true"
    }
  }
}
